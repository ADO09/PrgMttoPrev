generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file: db/dbSQLITE.db"
}



model MttosTable {
  idMTTO     Int      @id @default(autoincrement())
  idLinea    Int?
  idPlanta   Int?
  Nserie     String?
  comentarios String?
  problemasEncontrados String?
  cambiosRealizados String?
  path_IMG String?
  FechaPROG  String?
  FechaRealiz String?
  RealizadoPor String?
  RevisadoPor String?
  AprobadoPor String?
  estatus String?
  estacion String?
  planta String?
  path_excel String?
  path_pdf String?
  
}
